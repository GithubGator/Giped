<?xml version="1.0" encoding="utf-8" ?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="DECRET">
    <xs:complexType>
      <xs:sequence>
	<xs:element name="INTRO">
    	   <xs:complexType>
      	     <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="1" name="VERSIONXSD">
                   <!--Version du schema xsd -->
                   <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:pattern value="2.0"/>
                      </xs:restriction>
                   </xs:simpleType>
                </xs:element>
        	<xs:element maxOccurs="1" minOccurs="1" name="DATEXPORT">
                  <!--Date de generation du fichier XML -->
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
		       <xs:pattern value="()|(0000-00-00)|(9999-99-99)|((19)[0-9]{2}|(20)[0-9]{2})-((0)[1-9]{1}|(1)[0-2]{1})-((0)[1-9]{1}|(1)[0-9]{1}|(2)[0-9]{1}|(3)[0,1]{1})"/>
                     </xs:restriction>
                  </xs:simpleType>
                </xs:element>
      	     </xs:sequence>
	    </xs:complexType>
	</xs:element>
	<xs:element minOccurs="0" maxOccurs="unbounded" name="DONNEE">
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="0" maxOccurs="1" name="ACCFAM"> <!--Accompagnement social ou médico-social en cours d'au moins un membre du ménage. --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="ACCMOD"> <!--Caractère modulable de l'accueil --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="ALLOC"> <!--Allocations ou pensions liées à une situation d'invalidité ou de handicap --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="ANAIS"> <!--Année de naissance du mineur/majeur --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="(9999)|()|(0000)|(19)[0-9]{2}|(20)[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="ANSA1"> <!--Année de naissance de l'adulte 1 --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="(9999)|()|(0000)|(19)[0-9]{2}|(20)[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="ANSA2"> <!--Année de naissance de l'adulte 2 --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="(9999)|()|(0000)|(19)[0-9]{2}|(20)[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="ANSMERE"> <!--Année de naissance de la mère --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="(9999)|()|(0000)|(19)[0-9]{2}|(20)[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="ANSPERE"> <!--Année de naissance du père --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="(9999)|()|(0000)|(19)[0-9]{2}|(20)[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="AUTREDA"> <!--Si autre décision administrative de protection de l'enfance, préciser la décision --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:maxLength value="40" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="AUTREDJ"> <!--Si autre décision judiciaire de protection de l'enfance, préciser la décision --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:maxLength value="40" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="AUTREHEBER"> <!--Autre hébergement régulier du mineur le cas échéant --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="AUTRLIEUACC"> <!--Existence d'un autre lieu d'accueil régulier du mineur/majeur --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="AUTRLIEUAR"> <!--Existence d'un autre lieu d'accueil régulier du mineur/majeur --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="CHGLIEU"> <!--En cas de renouvellement d'une mesure de placement ou d'une prestation d'accueil du mineur, préciser si le mineur a changé de lieu principal d'accueil/placement lors de ce renouvellement (concerne uniquement les mineurs) --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="COMPOMENAG"> <!--Composition du ménage au sein du lieu de résidence principale du mineur --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Mineur autonome -->
                    <xs:enumeration value="2" /><!-- Parents vivant ensemble -->
                    <xs:enumeration value="3" /><!-- Mineur vivant avec sa mère seule -->
                    <xs:enumeration value="4" /><!-- Mineur vivant avec son père seul -->
                    <xs:enumeration value="5" /><!-- Résidence alternée -->
                    <xs:enumeration value="6" /><!-- Mineur vivant avec sa mère dans une famille recomposée -->
                    <xs:enumeration value="7" /><!-- Mineur vivant avec son père dans une famille recomposée -->
                    <xs:enumeration value="8" /><!-- Mineur vivant chez un autre membre de la famille -->
                    <xs:enumeration value="9" /><!-- Mineur vivant chez un particulier -->
                    <xs:enumeration value="10" /><!-- Autre -->
                    <xs:enumeration value="99" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="CONDADD"> <!--Conduite addictive (alcool ou drogue) d'un ou des adultes ayant en charge le mineur dans le lieu de résidence principale. --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui, avec prise en charge spécialisée connue -->
                    <xs:enumeration value="3" /><!-- Oui, sans prise en charge connue -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="CONDEDEV"> <!--Conditions de développement physique, intellectuel, affectif ou social gravement compromises ou en risque de l'être. --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="CONDEDUC"> <!--Conditions d'éducations gravement compromises ou en risque de l'être. --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="CONFL"> <!--Exposition du mineur à un conflit de couple. --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="CSPA1"> <!--Catégorie socioprofessionnelle de l'adulte 1 (selon la nomenclature INSEE) --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Agriculteurs exploitants -->
                    <xs:enumeration value="2" /><!-- Artisans, commerçants et chefs d'entreprise -->
                    <xs:enumeration value="3" /><!-- Cadres et professions intellectuelles supérieures -->
                    <xs:enumeration value="4" /><!-- Professions intermédiaires -->
                    <xs:enumeration value="5" /><!-- Employés -->
                    <xs:enumeration value="6" /><!-- Ouvriers -->
                    <xs:enumeration value="7" /><!-- Retraités -->
                    <xs:enumeration value="8" /><!-- Autres personnes sans activité professionnelle -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="CSPA2"> <!--Catégorie socioprofessionnelle de l'adulte 2 (selon la nomenclature INSEE) --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Agriculteurs exploitants -->
                    <xs:enumeration value="2" /><!-- Artisans, commerçants et chefs d'entreprise -->
                    <xs:enumeration value="3" /><!-- Cadres et professions intellectuelles supérieures -->
                    <xs:enumeration value="4" /><!-- Professions intermédiaires -->
                    <xs:enumeration value="5" /><!-- Employés -->
                    <xs:enumeration value="6" /><!-- Ouvriers -->
                    <xs:enumeration value="7" /><!-- Retraités -->
                    <xs:enumeration value="8" /><!-- Autres personnes sans activité professionnelle -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="CSPJM"> <!--Catégorie socioprofessionnelle du majeur (selon la nomenclature INSEE) --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Agriculteurs exploitants -->
                    <xs:enumeration value="2" /><!-- Artisans, commerçants et chefs d'entreprise -->
                    <xs:enumeration value="3" /><!-- Cadres et professions intellectuelles supérieures -->
                    <xs:enumeration value="4" /><!-- Professions intermédiaires -->
                    <xs:enumeration value="5" /><!-- Employés -->
                    <xs:enumeration value="6" /><!-- Ouvriers -->
                    <xs:enumeration value="7" /><!-- Retraités -->
                    <xs:enumeration value="8" /><!-- Autres personnes sans activité professionnelle -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="DANGER"> <!--Mise en danger du mineur par lui-même --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="DATDCMERE"> <!--Si décès de la mère, année et mois du décès. 9999 si année inconnue, 99 si mois inconnu --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
			<xs:pattern value="()|(0000-00)|((9999)|(19)[0-9]{2}|(20)[0-9]{2})-((99)|(0)[1-9]{1}|(1)[0-2]{1})" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="DATDCPERE"> <!--Si décès du père, année et mois du décès. 9999 si année inconnue, 99 si mois inconnu --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
			<xs:pattern value="()|(0000-00)|((9999)|(19)[0-9]{2}|(20)[0-9]{2})-((99)|(0)[1-9]{1}|(1)[0-2]{1})" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="DATDEB"> <!--Date de début effective de la prestation ou de la mesure de protection de l’enfance.9999 si année inconnue, 99 si mois inconnu et 99 si jour inconnu --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="()|(0000-00-00)|((19)[0-9]{2}|(20)[0-9]{2})-((0)[1-9]{1}|(1)[0-2]{1})-((0)[1-9]{1}|(1)[0-9]{1}|(2)[0-9]{1}|(3)[0,1]{1})" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="DATDECAP"> <!--Date de la décision relative à l'autorité parentale.9999 si année inconnue, 99 si mois inconnu et 99 si jour inconnu. --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="()|(0000-00-00)|((19)[0-9]{2}|(20)[0-9]{2})-((0)[1-9]{1}|(1)[0-2]{1})-((0)[1-9]{1}|(1)[0-9]{1}|(2)[0-9]{1}|(3)[0,1]{1})" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="DATDECMIN"> <!--Si décès du mineur/majeur, année et mois du décès --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
			<xs:pattern value="()|(0000-00)|((9999)|(19)[0-9]{2}|(20)[0-9]{2})-((99)|(0)[1-9]{1}|(1)[0-2]{1})" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="DATDECPE"> <!--Date de la décision de protection de l'enfance. 9999 si année inconnue, 99 si mois inconnu et 99 si jour inconnu --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="()|(0000-00-00)|((19)[0-9]{2}|(20)[0-9]{2})-((0)[1-9]{1}|(1)[0-2]{1})-((0)[1-9]{1}|(1)[0-9]{1}|(2)[0-9]{1}|(3)[0,1]{1})" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="DATFIN"> <!--Date de fin effective de la prestation ou de la mesure de protection de l’enfance. 9999 si année inconnue, 99 si mois inconnu et 99 si jour inconnu --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="()|(0000-00-00)|((19)[0-9]{2}|(20)[0-9]{2})-((0)[1-9]{1}|(1)[0-2]{1})-((0)[1-9]{1}|(1)[0-9]{1}|(2)[0-9]{1}|(3)[0,1]{1})" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="DATIP"> <!--Date de réception de l'information initiale sur la situation de danger ou de risque de danger du mineur/majeur à la CRIP ou dans les services du conseil départemental --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="()|(0000-00-00)|((19)[0-9]{2}|(20)[0-9]{2})-((0)[1-9]{1}|(1)[0-2]{1})-((0)[1-9]{1}|(1)[0-9]{1}|(2)[0-9]{1}|(3)[0,1]{1})" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="DATJE"> <!--Date de la saisine du juge des enfants. 9999 si année inconnue, 99 si mois inconnu et 99 si jour inconnu --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="()|(0000-00-00)|((19)[0-9]{2}|(20)[0-9]{2})-((0)[1-9]{1}|(1)[0-2]{1})-((0)[1-9]{1}|(1)[0-9]{1}|(2)[0-9]{1}|(3)[0,1]{1})" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="DATPPE"> <!--S'il existe un PPE, Date d'établissement du PPE. 9999 si année inconnue, 99 si mois inconnu et 99 si jour inconnu --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="()|(0000-00-00)|((19)[0-9]{2}|(20)[0-9]{2})-((0)[1-9]{1}|(1)[0-2]{1})-((0)[1-9]{1}|(1)[0-9]{1}|(2)[0-9]{1}|(3)[0,1]{1})" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="DATSIGN"> <!--Date du signalement direct auprès du procureur de la république. 9999 si année inconnue, 99 si mois inconnu et 99 si jour inconnu --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="()|(0000-00-00)|((19)[0-9]{2}|(20)[0-9]{2})-((0)[1-9]{1}|(1)[0-2]{1})-((0)[1-9]{1}|(1)[0-9]{1}|(2)[0-9]{1}|(3)[0,1]{1})" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="DCMERE"> <!--Mère décédée --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="DCPERE"> <!--Père décédé --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="DECAP"> <!--Décision relative à l'autorité parentale --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Décision du juge aux affaires familiales sur l'exercice de l'autorité parentale -->
                    <xs:enumeration value="2" /><!-- Délégation de l'autorité parentale -->
                    <xs:enumeration value="3" /><!-- Retrait -->
                    <xs:enumeration value="4" /><!-- Tutelle -->
                    <xs:enumeration value="5" /><!-- Déclaration judiciaire de délaissement parental -->
                    <xs:enumeration value="6" /><!-- Pas de décision relative à l'autorité parentale -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="DECISION"> <!--Nature de la décision de protection de l'enfance --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Décision administrative en protection de l'enfance -->
                    <xs:enumeration value="2" /><!-- Décision judiciaire en protection de l'enfance -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="DEFINTEL"> <!--Situation de handicap psychique ou mental reconnue par la MDPH d'un ou des adultes ayant en charge le mineur dans le lieu de résidence principale --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="DIPLOME"> <!--A la fin de la mesure ou prestation de protection de l'enfance, dernier diplôme obtenu par le mineur/majeur --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Aucun diplôme -->
                    <xs:enumeration value="2" /><!-- Brevet -->
                    <xs:enumeration value="3" /><!-- CAP, BEP ou équivalent -->
                    <xs:enumeration value="4" /><!-- Baccalauréat ou équivalent -->
                    <xs:enumeration value="5" /><!-- Diplôme du supérieur -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="1" name="EMPLA1"> <!--Situation face à l'emploi de l'adulte 1 (selon la nomenclature INSEE) --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- En contrat d'apprentissage -->
                    <xs:enumeration value="2" /><!-- Placé par une agence d'intérim -->
                    <xs:enumeration value="3" /><!-- Emploi aidé -->
                    <xs:enumeration value="4" /><!-- Stagiaire rémunéré -->
                    <xs:enumeration value="5" /><!-- Emploi à durée limitée (CDD, contrat court, saisonnier, vacataire...) -->
