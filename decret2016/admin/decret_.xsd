<?xml version="1.0" encoding="utf-8" ?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="DECRET">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" name="donnee">
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="0" name="ACCFAM"> <!--Accompagnement social ou médico-social en cours d'au moins un membre de la famille. --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="ACCMOD"> <!--L’accueil est-il modulable ? --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="ALLOC"> <!--le ménage perçoit-il des allocations ou des pensions. --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="ANAIS"> <!--Année de naissance --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="ANSA1"> <!--Année de naissance de l'adulte 1 --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="ANSA2"> <!--Année de naissance de l'adulte 2 --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="ANSMERE"> <!--Année de naissance de la mère --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="ANSPERE"> <!--Année de naissance du père --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="AUTRE"> <!--Le ménage dispose-t-il d'autres sources de revenus --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- OUI -->
                    <xs:enumeration value="2" /><!-- NON -->
                    <xs:enumeration value="9" /><!-- NE SAIT PAS -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="AUTREDA"> <!--Si autre décision administrative de protection de l'enfance, préciser la décision --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:maxLength value="40" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="AUTREDJ"> <!--Si autre décision judiciaire en  assistance éducative, préciser. --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:maxLength value="40" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="AUTREHEBER"> <!--Autre hébergement régulier du mineur --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="AUTRLIEUACC"> <!--Existence d'un autre lieu d'accueil régulier du mineur --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="AUTRLIEUAR"> <!--Le mineur a-t-il un autre lieu d'accueil régulier ? --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="CODEV"> <!--Codification du type d'évènement --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Evaluation de l'information préoccupante donnant lieu à une entrée ou un maintien dans la protection de l'enfance -->
                    <xs:enumeration value="2" /><!-- Signalement direct donnant lieu à une entrée ou à un maintien dans la protection de l'enfance -->
                    <xs:enumeration value="3" /><!-- Saisie directe du juge des enfants -->
                    <xs:enumeration value="4" /><!-- Mesure de protection de l'enfance -->
                    <xs:enumeration value="5" /><!-- Renouvellement ou fin de l'intervention en protection de l'enfance -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="COMPOMENAG"> <!--Composition du ménage --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Mineur autonome -->
                    <xs:enumeration value="2" /><!-- Parents vivant ensemble -->
                    <xs:enumeration value="3" /><!-- Mineur vivant avec sa mère seule -->
                    <xs:enumeration value="4" /><!-- Mineur vivant avec son père seul -->
                    <xs:enumeration value="5" /><!-- Résidence alternée -->
                    <xs:enumeration value="6" /><!-- Mineur vivant avec sa mère dans une famille recomposée -->
                    <xs:enumeration value="7" /><!-- Mineur vivant avec son père dans une famille recomposée -->
                    <xs:enumeration value="8" /><!-- Mineur vivant chez un autre membre de la famille -->
                    <xs:enumeration value="9" /><!-- Mineur vivant chez un particulier -->
                    <xs:enumeration value="10" /><!-- Autre -->
                    <xs:enumeration value="99" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="CONDADD"> <!--Conduite addictive (alcool ou drogue) d'un ou des adultes ayant en charge le mineur dans le lieu de résidence principale. --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui, avec prise en charge spécialisée connue -->
                    <xs:enumeration value="3" /><!-- Oui, sans prise en charge connue -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="CONDEDEV"> <!--Conditions de développement physique, intellectuel, affectif ou social gravement compromises ou en risque de l'être. --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="CONDEDUC"> <!--Conditions d'éducations gravement compromises ou en risque de l'être. --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="CONFL"> <!--Exposition du mineur à un conflit de couple. --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="CONTMERE"> <!--Quelle est la fréquence des contacts de la mère avec le mineur --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Au moins une fois par semaine -->
                    <xs:enumeration value="2" /><!-- Au moins une fois par mois -->
                    <xs:enumeration value="3" /><!-- Au moins une fois tous les 6 mois -->
                    <xs:enumeration value="4" /><!-- Au moins une fois par an -->
                    <xs:enumeration value="5" /><!-- Aucun contact -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="CONTPERE"> <!--Quelle est la fréquence des contacts du père avec le mineur ? --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Au moins une fois par semaine -->
                    <xs:enumeration value="2" /><!-- Au moins une fois par mois -->
                    <xs:enumeration value="3" /><!-- Au moins une fois tous les 6 mois -->
                    <xs:enumeration value="4" /><!-- Au moins une fois par an -->
                    <xs:enumeration value="5" /><!-- Aucun contact -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="CSPA1"> <!--Catégorie socioprofessionnelle de l’adulte1 (selon la nomenclature INSEE) --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Agriculteurs exploitants -->
                    <xs:enumeration value="2" /><!-- Artisans, commerçants et chefs d'entreprise -->
                    <xs:enumeration value="3" /><!-- Cadres et professions intellectuelles supérieures -->
                    <xs:enumeration value="4" /><!-- Professions intermédiaires -->
                    <xs:enumeration value="5" /><!-- Employés -->
                    <xs:enumeration value="6" /><!-- Ouvriers -->
                    <xs:enumeration value="7" /><!-- Retraités -->
                    <xs:enumeration value="8" /><!-- Autres personnes sans activité professionnelle -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="CSPA2"> <!--Catégorie socioprofessionnelle de l’adulte2 (selon la nomenclature INSEE) --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Agriculteurs exploitants -->
                    <xs:enumeration value="2" /><!-- Artisans, commerçants et chefs d'entreprise -->
                    <xs:enumeration value="3" /><!-- Cadres et professions intellectuelles supérieures -->
                    <xs:enumeration value="4" /><!-- Professions intermédiaires -->
                    <xs:enumeration value="5" /><!-- Employés -->
                    <xs:enumeration value="6" /><!-- Ouvriers -->
                    <xs:enumeration value="7" /><!-- Retraités -->
                    <xs:enumeration value="8" /><!-- Autres personnes sans activité professionnelle -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATAVIS"> <!--En cas d'ouverture directe d'une procédure auprès du juge des enfants : date d'avis d'ouverture de la procédure. --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATDCMERE"> <!--Si décédée, mois et année du décès de la mère --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATDCPERE"> <!--Si décédé, mois et année du décès du père --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATDEBACC"> <!--Date de début d’intervention de décision administrative d'accueil provisoire --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATDEBAD"> <!--Date de début d’intervention --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATDEBINTER"> <!--Date de début d’intervention si décision judiciaire d'action éducative en milieu ouvert ou d'investigation --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATDEBPLAC"> <!--Date de début d’intervention --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATDECAP"> <!--Date de la décision relative à l'autorité parentale --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATDECMDPH"> <!--Si prise en charge spécifique, date de la décision de la commission des droits et de l'autonomie. --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATDECMIN"> <!--Si décès du mineur, mois et année du décès --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATDECPE"> <!--Date de la décision de protection de l'enfance. --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATEXDECMDPH"> <!--Si prise en charge spécifique, date d'exécution de la décision de la commission des droits et de l'autonomie. --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATFINACC"> <!--Date de fin d’intervention  de la décision administrative d'accueil provisoire --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATFINAD"> <!--Date de fin d’intervention --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATFININTER"> <!--Date de fin d’intervention --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATFINPLAC"> <!--Date de fin d’intervention --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATIP"> <!--Date de réception de l'information préoccupante --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATJE"> <!--Date de la saisine directe du juge des enfants --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATSIGN"> <!--Date du signalement direct auprès du procureur de la république --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DATSIGNPROJ"> <!--Date de la signature du projet pour l'enfant --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                   <xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DCMERE"> <!--Décès de la mère --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DCPERE"> <!--Décès du père --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DECAP"> <!--Décision relative à l’autorité parentale --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Décision du juge aux affaires familiales sur l'exercice de l'autorité parentale -->
                    <xs:enumeration value="2" /><!-- Délégation de l'autorité parentale -->
                    <xs:enumeration value="3" /><!-- Retrait -->
                    <xs:enumeration value="4" /><!-- Tutelle -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DECISION"> <!--Nature de la décision de protection de l'enfance --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Décision administrative en protection de l'enfance -->
                    <xs:enumeration value="2" /><!-- Décision judiciaire en assistance éducative -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DEFINTEL"> <!--Déficience intellectuelle ou mentale reconnue par la maison départementale des personnes handicapées d'un ou des adultes ayant en charge le mineur dans le lieu de résidence principal. --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Non -->
                    <xs:enumeration value="2" /><!-- Oui -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="DIPLOME"> <!--Dernier diplôme obtenu par le mineur à la fin de l'intervention en protection de l'enfance --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="1" /><!-- Aucun diplôme -->
                    <xs:enumeration value="2" /><!-- Brevet -->
                    <xs:enumeration value="3" /><!-- CAP, BEP ou équivalent -->
                    <xs:enumeration value="4" /><!-- Baccalauréat ou équivalent -->
                    <xs:enumeration value="5" /><!-- Diplôme du supérieur -->
                    <xs:enumeration value="9" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="EMPLA1"> <!--Situation face à l'emploi de l'adulte 1 --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="10" /><!-- Salarié contrat à durée indéterminée -->
                    <xs:enumeration value="11" /><!-- A son compte ou aidant un membre de sa famille dans son travail -->
                    <xs:enumeration value="12" /><!-- Contrat à durée déterminée, mission d'intérim, apprentissage, travail saisonnier -->
                    <xs:enumeration value="13" /><!-- Stage de formation professionnelle, ou contrat d'aide à l'emploi -->
                    <xs:enumeration value="20" /><!-- Au chomage -->
                    <xs:enumeration value="30" /><!-- Elève, étudiant, stagiaire non rémunéré -->
                    <xs:enumeration value="40" /><!-- Militaire du contingent -->
                    <xs:enumeration value="50" /><!-- Autre situation : retraité, préretraité, femme au foyer, autre... -->
                    <xs:enumeration value="99" /><!-- Ne sait pas -->
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="EMPLA2"> <!--Situation face à l'emploi de l'adulte 2 --> 
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="" />
                    <xs:enumeration value="10" /><!-- Salarié contrat à durée indéterminée -->
                    <xs:enumeration value="11" /><!-- A son compte ou aidant un membre de sa famille dans son travail -->
                    <xs:enumeration value="12" /><!-- Contrat à durée déterminée, mission d'intérim, apprentissage, travail saisonnier -->
                    <xs:enumeration value="13" /><!-- Stage de formation professionnelle, ou contrat d'aide à l'emploi -->
                    <xs:enumeration value="20" /><!-- Au chomage -->
                    <xs:enumeration value="30" /><!-- Elève, étudiant, stagiaire non rémunéré -->
                    <xs:enumeration value="40" /><!-- Militaire du contingent -->
